{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt generate",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest",
    "prepare": "nuxt prepare && husky install"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "npm:@nuxt/content-edge@latest",
    "prism-themes": "1.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.4",
    "@commitlint/config-conventional": "16.2.4",
    "@nuxtjs/color-mode": "3.0.2",
    "@nuxtjs/eslint-config": "9.0.0",
    "@nuxtjs/eslint-config-typescript": "9.0.0",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/tailwindcss": "^5.0.2",
    "c8": "^7.11.0",
    "eslint": "8.11.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "3.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.5",
    "nuxt": "npm:nuxt3@latest",
    "postcss-html": "1.3.0",
    "prettier": "2.5.1",
    "stylelint": "14.6.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "25.0.0",
    "vitest": "^0.6.1",
    "vue-use-web": "1.0.1"
  },
  "packageManager": "pnpm@6.32.6"
}
