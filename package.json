{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt generate",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest run test/unit",
    "test:e2e": "playwright test test/e2e",
    "postinstall": "nuxt prepare && husky install"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "npm:@nuxt/content-edge@2.1.0-27658086.63749d2",
    "@nuxt/kit": "npm:@nuxt/kit-edge@3.0.0-rc.6-27661960.60656df",
    "critters": "^0.0.16",
    "prism-themes": "1.9.0",
    "vue-plausible": "^1.3.2",
    "nuxt-full-static": "latest"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@nuxtjs/color-mode": "3.1.4",
    "@nuxtjs/eslint-config": "10.0.0",
    "@nuxtjs/eslint-config-typescript": "10.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/tailwindcss": "^5.3.1",
    "@playwright/test": "^1.24.2",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/test-utils": "^2.0.0",
    "c8": "^7.12.0",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "3.2.0",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "^6.0.4",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "magic-regexp": "^0.4.1",
    "nuxt": "npm:nuxt3@3.0.0-rc.6-27661960.60656df",
    "postcss-html": "1.5.0",
    "prettier": "2.7.1",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "26.0.0",
    "vitest": "^0.21.0"
  },
  "packageManager": "pnpm@7.8.0"
}
