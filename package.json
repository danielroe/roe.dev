    "test:e2e": "playwright test test/e2e",
    "postinstall": "nuxt prepare && husky install && node ./node_modules/playwright/install.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,vue}": "stylelint"
