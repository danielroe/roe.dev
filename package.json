{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "partykit:dev": "partykit dev ./partykit/server.ts",
    "partykit:deploy": "partykit deploy ./partykit/server.ts",
    "lint:js": "eslint .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest run test/unit/behaviour && vitest run test/unit/bundle && vitest run test/nuxt --config vitest.nuxt.config.mts",
    "test:types": "nuxi prepare && vue-tsc --noEmit",
    "test:e2e": "playwright test test/e2e",
    "test:e2e:update": "docker run --rm --network host -v $(pwd):/work/ -v /tmp/playwright-nm:/work/node_modules -w /work/ -it mcr.microsoft.com/playwright:v1.52.0-noble bash -c 'corepack enable && pnpm i && pnpm playwright test test/e2e --update-snapshots'",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@atproto/api": "^0.15.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/svg-spinners": "^1.2.2",
    "@iconify-json/tabler": "^1.2.17",
    "@nuxt/content": "3.5.1",
    "@nuxt/eslint": "1.3.1",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/image": "1.10.0",
    "@nuxt/scripts": "^0.11.1",
    "@nuxtjs/color-mode": "3.5.2",
    "@nuxtjs/html-validator": "2.1.0",
    "@nuxtjs/plausible": "1.2.0",
    "@nuxtjs/sanity": "^1.13.3",
    "@unhead/vue": "2.0.8",
    "@unocss/nuxt": "^66.0.0",
    "@unocss/postcss": "^66.0.0",
    "@unocss/reset": "^66.0.0",
    "defu": "^6.1.4",
    "exsolve": "^1.0.4",
    "feed": "^5.0.0",
    "globby": "^14.1.0",
    "gray-matter": "4.0.3",
    "h3": "^1.15.1",
    "image-meta": "^0.2.1",
    "knitwork": "^1.2.0",
    "magic-regexp": "0.10.0",
    "magic-string": "^0.30.17",
    "masto": "^7.0.0",
    "mlly": "^1.7.4",
    "mocked-exports": "^0.1.0",
    "nuxt": "^3.16.0",
    "nuxt-og-image": "5.1.3",
    "nuxt-security": "2.2.0",
    "partykit": "0.0.114",
    "partysocket": "^1.0.3",
    "pathe": "^2.0.3",
    "postcss-nesting": "^13.0.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "std-env": "^3.8.1",
    "ufo": "^1.5.4",
    "unocss": "^66.0.0",
    "unplugin": "^2.2.0",
    "vue": "3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@nuxt/test-utils": "3.18.0",
    "@playwright/test": "1.52.0",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.27.0",
    "happy-dom": "17.4.6",
    "lint-staged": "16.0.0",
    "ofetch": "1.4.1",
    "playwright-core": "1.52.0",
    "rollup": "4.40.2",
    "simple-git-hooks": "2.13.0",
    "stylelint": "16.19.1",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "38.0.0",
    "typescript": "5.8.3",
    "vitest": "3.1.3",
    "vue-tsc": "2.2.10"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npx lint-staged"
  },
  "packageManager": "pnpm@10.11.0",
  "pnpm": {
    "overrides": {
      "@nuxt/kit": "3.17.3",
      "@nuxt/schema": "3.17.3",
      "@nuxtjs/mdc": "0.17.0",
      "vue": "3.5.13"
    },
    "patchedDependencies": {
      "@nuxt/image": "patches/@nuxt__image.patch"
    }
  }
}
