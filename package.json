{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "node scripts/build.mjs && nuxt build",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "echo 'need some unit tests' || vitest run test/unit",
    "test:types": "nuxi typecheck",
    "test:e2e": "playwright test test/e2e",
    "postinstall": "nuxt prepare && husky install && node ./node_modules/playwright/install.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "2.2.2",
    "@nuxt/image-edge": "1.0.0-27809804.194d2d6",
    "@nuxt/kit": "npm:@nuxt/kit-edge@3.0.1-rc.0-27810184.d991a55",
    "@nuxtjs/fontaine": "0.2.2",
    "@nuxtjs/web-vitals": "0.1.10",
    "gray-matter": "4.0.3",
    "jose": "4.11.0",
    "nuxt-plausible": "0.1.2",
    "prism-themes": "1.9.0",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@nuxtjs/color-mode": "3.1.8",
    "@nuxtjs/eslint-config": "11.0.0",
    "@nuxtjs/eslint-config-typescript": "11.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/html-validator": "1.1.1",
    "@nuxtjs/tailwindcss": "6.1.3",
    "@playwright/test": "1.28.0",
    "@types/node": "18.11.9",
    "@vitejs/plugin-vue": "3.2.0",
    "@vue/test-utils": "2.2.4",
    "c8": "7.12.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "7.7.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "magic-regexp": "0.6.1",
    "nuxt": "npm:nuxt3@3.0.1-rc.0-27810184.d991a55",
    "playwright": "1.28.0",
    "postcss-html": "1.5.0",
    "prettier": "2.7.1",
    "stylelint": "14.15.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "29.0.0",
    "vitest": "0.25.2"
  },
  "packageManager": "pnpm@7.16.1"
}
