{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "partykit:dev": "partykit dev ./partykit/server.ts",
    "partykit:deploy": "partykit deploy ./partykit/server.ts",
    "lint:js": "eslint .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest run test/unit/behaviour && vitest run test/unit/bundle && vitest run test/nuxt --config vitest.nuxt.config.mts",
    "test:types": "nuxi prepare && vue-tsc --noEmit",
    "test:e2e": "playwright test test/e2e",
    "test:e2e:update": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.47.0-focal npx playwright test test/e2e --update-snapshots",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@atproto/api": "^0.13.7",
    "@iconify-json/ri": "^1.2.0",
    "@iconify-json/svg-spinners": "^1.2.0",
    "@iconify-json/tabler": "^1.2.2",
    "@nuxt/content": "2.13.2",
    "@nuxt/eslint": "0.5.7",
    "@nuxt/fonts": "0.8.0",
    "@nuxt/image": "1.8.0",
    "@nuxtjs/color-mode": "3.4.4",
    "@nuxtjs/html-validator": "1.8.2",
    "@nuxtjs/plausible": "1.0.2",
    "@nuxtjs/web-vitals": "0.2.7",
    "@unhead/vue": "^1.10.4",
    "@unocss/nuxt": "^0.62.3",
    "@unocss/postcss": "^0.62.3",
    "@unocss/reset": "^0.62.3",
    "defu": "^6.1.4",
    "feed": "^4.2.2",
    "globby": "^14.0.2",
    "gray-matter": "4.0.3",
    "h3": "^1.12.0",
    "image-meta": "^0.2.1",
    "knitwork": "^1.1.0",
    "magic-regexp": "0.8.0",
    "magic-string": "^0.30.11",
    "masto": "^6.8.0",
    "mlly": "^1.7.1",
    "nuxt": "^3.13.1",
    "nuxt-og-image": "3.0.2",
    "nuxt-security": "2.0.0-rc.9",
    "nuxt-time": "^0.1.3",
    "partykit": "0.0.110",
    "partysocket": "^1.0.2",
    "pathe": "^1.1.2",
    "postcss-nesting": "^13.0.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "std-env": "^3.7.0",
    "ufo": "^1.5.4",
    "unenv": "^1.10.0",
    "unocss": "^0.62.3",
    "unplugin": "^1.14.1",
    "vue": "^3.5.4",
    "vue-router": "^4.4.4"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@nuxt/test-utils": "3.14.2",
    "@playwright/test": "1.47.0",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.10.0",
    "execa": "9.3.1",
    "happy-dom": "15.7.4",
    "lint-staged": "15.2.10",
    "ofetch": "1.3.4",
    "playwright-core": "1.47.0",
    "rollup": "4.21.3",
    "simple-git-hooks": "2.11.1",
    "stylelint": "16.9.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.1",
    "typescript": "5.6.2",
    "vitest": "2.1.0",
    "vue-tsc": "2.1.6"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npx lint-staged"
  },
  "packageManager": "pnpm@9.10.0",
  "pnpm": {
    "patchedDependencies": {
      "nitropack@2.9.7": "patches/nitropack@2.9.6.patch"
    },
    "overrides": {
      "@nuxt/kit": "3.13.1",
      "@nuxt/schema": "3.13.1",
      "@nuxtjs/mdc": "0.8.3"
    }
  }
}
