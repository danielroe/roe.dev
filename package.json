{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest run test/unit",
    "test:types": "nuxi typecheck",
    "test:e2e": "playwright test test/e2e",
    "postinstall": "nuxt prepare && husky install && node ./node_modules/playwright/install.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "2.4.1",
    "@nuxt/image-edge": "1.0.0-27907872.f4dd19d",
    "@nuxt/kit": "npm:@nuxt/kit-edge@3.0.1-rc.0-27909581.f711046",
    "@nuxtjs/color-mode": "3.2.0",
    "@nuxtjs/fontaine": "0.2.4",
    "@nuxtjs/html-validator": "1.2.3",
    "@nuxtjs/plausible": "0.2.0",
    "@nuxtjs/tailwindcss": "6.3.1",
    "@nuxtjs/web-vitals": "0.2.2",
    "feed": "^4.2.2",
    "gray-matter": "4.0.3",
    "jose": "4.11.2",
    "magic-regexp": "0.6.2",
    "nuxt": "npm:nuxt3@3.0.1-rc.0-27909581.f711046",
    "playwright": "1.30.0",
    "prism-themes": "1.9.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@playwright/test": "1.30.0",
    "@types/node": "18.11.18",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/test-utils": "2.2.10",
    "c8": "7.12.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "8.2.6",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "nuxt-vitest": "0.6.5",
    "postcss-html": "1.5.0",
    "prettier": "2.8.3",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "29.0.0",
    "vitest": "0.28.4"
  },
  "packageManager": "pnpm@7.26.3"
}
