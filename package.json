{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "partykit:dev": "partykit dev ./partykit/server.ts",
    "partykit:deploy": "partykit deploy ./partykit/server.ts",
    "lint:js": "eslint --ext .js,.ts,.mjs,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest run test/unit",
    "test:types": "nuxi prepare && vue-tsc --noEmit",
    "test:e2e": "playwright test test/e2e",
    "test:e2e:update": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.36.1-focal npx playwright test test/e2e --update-snapshots",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install && npx playwright install"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@atproto/api": "^0.6.20",
    "@iconify-json/ri": "^1.1.12",
    "@iconify-json/svg-spinners": "^1.1.2",
    "@nuxt/content": "2.8.5",
    "@nuxt/image": "1.0.0-rc.3",
    "@nuxt/kit": "3.7.4",
    "@nuxtjs/color-mode": "3.3.0",
    "@nuxtjs/fontaine": "0.4.1",
    "@nuxtjs/html-validator": "1.5.2",
    "@nuxtjs/plausible": "0.2.3",
    "@nuxtjs/web-vitals": "0.2.6",
    "@unocss/nuxt": "^0.56.5",
    "@unocss/postcss": "^0.56.5",
    "feed": "^4.2.2",
    "globby": "^13.2.2",
    "gray-matter": "4.0.3",
    "knitwork": "^1.0.0",
    "magic-regexp": "0.7.0",
    "magic-string": "^0.30.5",
    "masto": "^5.11.4",
    "mlly": "^1.4.2",
    "nuxt": "3.7.4",
    "nuxt-time": "^0.1.1",
    "partykit": "0.0.30",
    "pathe": "^1.1.1",
    "postcss-nesting": "^12.0.1",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ufo": "^1.3.1",
    "unplugin": "^1.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@nuxt/devtools": "0.8.5",
    "@nuxt/eslint-config": "0.2.0",
    "@nuxt/test-utils": "3.7.4",
    "@playwright/test": "1.39.0",
    "@vue/test-utils": "2.4.1",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "happy-dom": "12.9.1",
    "lint-staged": "14.0.1",
    "nuxt-vitest": "0.11.0",
    "ofetch": "1.3.3",
    "prettier": "3.0.3",
    "simple-git-hooks": "2.9.0",
    "stylelint": "15.10.3",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "34.0.0",
    "typescript": "5.2.2",
    "vitest": "0.33.0",
    "vue-tsc": "1.8.19"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm commitlint --edit $1",
    "pre-commit": "pnpm lint-staged"
  },
  "packageManager": "pnpm@8.9.0",
  "pnpm": {
    "patchedDependencies": {
      "@nuxtjs/color-mode@3.3.0": "patches/@nuxtjs__color-mode@3.3.0.patch"
    }
  }
}
