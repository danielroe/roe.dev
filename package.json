{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "pnpm rebuild sharp && nuxt build",
    "start": "nuxt start",
    "partykit:dev": "partykit dev ./partykit/server.ts",
    "partykit:deploy": "partykit deploy ./partykit/server.ts",
    "lint:js": "eslint .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest run",
    "test:types": "nuxt prepare && vue-tsc -b --noEmit",
    "test:e2e": "playwright test test/e2e",
    "test:e2e:update": "docker run --rm --network host -v $(pwd):/work/ -v /tmp/playwright-nm:/work/node_modules -w /work/ -it mcr.microsoft.com/playwright:v1.57.0-noble bash -c 'corepack enable && pnpm i && pnpm playwright test test/e2e --update-snapshots'",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install",
    "auth:linkedin": "node --env-file=.env scripts/linkedin-auth.ts",
    "auth:youtube": "node --env-file=.env scripts/youtube-auth.ts",
    "auth:youtube:refresh": "node --env-file=.env scripts/youtube-auth.ts refresh",
    "import-streambeats": "node --env-file=.env scripts/import-streambeats.mjs",
    "import-streambeats:dry-run": "node --env-file=.env scripts/import-streambeats.mjs --dry-run"
  },
  "nano-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@atcute/bluesky-richtext-segmenter": "^3.0.0",
    "@atproto/api": "^0.18.21",
    "@iconify-json/ri": "^1.2.10",
    "@iconify-json/svg-spinners": "^1.2.4",
    "@iconify-json/tabler": "^1.2.26",
    "@nuxt/eslint": "1.15.1",
    "@nuxt/fonts": "0.14.0",
    "@nuxt/image": "2.0.0",
    "@nuxt/scripts": "^0.13.2",
    "@nuxtjs/color-mode": "^4.0.0",
    "@nuxtjs/html-validator": "2.1.0",
    "@nuxtjs/mdc": "0.20.1",
    "@nuxtjs/plausible": "3.0.1",
    "@nuxtjs/sanity": "^2.2.0",
    "@portabletext/types": "^4.0.1",
    "@sanity/client": "^7.15.0",
    "@sanity/webhook": "^4.0.4",
    "@unhead/vue": "2.1.4",
    "@unocss/nuxt": "66.6.0",
    "@unocss/postcss": "66.6.0",
    "@unocss/reset": "66.6.0",
    "@vercel/blob": "^2.2.0",
    "citty": "^0.2.0",
    "defu": "^6.1.4",
    "exsolve": "^1.0.8",
    "feed": "^5.2.0",
    "gray-matter": "4.0.3",
    "h3": "^1.15.5",
    "image-meta": "^0.2.2",
    "knitwork": "^1.3.0",
    "magic-regexp": "0.10.0",
    "magic-string": "^0.30.21",
    "masto": "^7.10.1",
    "mlly": "^1.8.0",
    "mocked-exports": "^0.1.1",
    "nuxt": "4.3.1",
    "nuxt-og-image": "5.1.13",
    "nuxt-security": "2.5.1",
    "partykit": "0.0.115",
    "partysocket": "^1.1.13",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "postcss-nesting": "^14.0.0",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "scule": "^1.3.0",
    "std-env": "^3.10.0",
    "tinyglobby": "^0.2.15",
    "ufo": "^1.6.3",
    "unocss": "66.6.0",
    "unplugin": "^3.0.0",
    "vue": "3.5.28",
    "vue-router": "5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "20.4.2",
    "@commitlint/config-conventional": "20.4.2",
    "@nuxt/test-utils": "4.0.0",
    "@playwright/test": "1.58.2",
    "@sanity/eslint-config-studio": "6.0.0",
    "@types/html-to-text": "9.0.4",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.39.2",
    "googleapis": "171.4.0",
    "happy-dom": "20.6.1",
    "html-to-text": "9.0.5",
    "nano-staged": "0.9.0",
    "ofetch": "1.5.1",
    "playwright-core": "1.58.2",
    "rollup": "4.58.0",
    "simple-git-hooks": "2.13.1",
    "stylelint": "17.3.0",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "40.0.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18",
    "vue-tsc": "3.2.4"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npx nano-staged"
  },
  "packageManager": "pnpm@10.30.1"
}
