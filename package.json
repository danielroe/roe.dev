{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "node scripts/build.mjs && nuxt build",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "echo 'need some unit tests' || vitest run test/unit",
    "test:types": "nuxi typecheck",
    "test:e2e": "playwright test test/e2e",
    "postinstall": "nuxt prepare && husky install && node ./node_modules/playwright/install.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "npm:@nuxt/content-edge@2.2.0-27781098.a7e4f27",
    "@nuxt/image-edge": "1.0.0-27769790.4b27db3",
    "@nuxt/kit": "npm:@nuxt/kit-edge@3.0.0-rc.13-27781436.1f6b3be",
    "@nuxtjs/fontaine": "0.2.2",
    "gray-matter": "4.0.3",
    "jose": "4.10.4",
    "nuxt-plausible": "0.1.1",
    "prism-themes": "1.9.0",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@nuxtjs/color-mode": "3.1.8",
    "@nuxtjs/eslint-config": "11.0.0",
    "@nuxtjs/eslint-config-typescript": "11.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/html-validator": "1.1.0",
    "@nuxtjs/tailwindcss": "6.1.2",
    "@playwright/test": "1.27.1",
    "@types/node": "18.11.9",
    "@vitejs/plugin-vue": "3.2.0",
    "@vue/test-utils": "2.2.1",
    "c8": "7.12.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "7.6.6",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "magic-regexp": "0.6.0",
    "nuxt": "npm:nuxt3@3.0.0-rc.13-27781436.1f6b3be",
    "playwright": "1.27.1",
    "postcss-html": "1.5.0",
    "prettier": "2.7.1",
    "stylelint": "14.14.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "29.0.0",
    "vitest": "0.24.4"
  },
  "packageManager": "pnpm@7.14.1"
}
