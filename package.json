{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "partykit:dev": "partykit dev ./partykit/server.ts",
    "partykit:deploy": "partykit deploy ./partykit/server.ts",
    "lint:js": "eslint --ext .js,.ts,.mjs,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest run test/unit",
    "test:types": "nuxi prepare && vue-tsc --noEmit",
    "test:e2e": "playwright test test/e2e",
    "test:e2e:update": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.36.1-focal npx playwright test test/e2e --update-snapshots",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install && npx playwright install"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@atproto/api": "^0.4.3",
    "@iconify-json/ri": "^1.1.10",
    "@iconify-json/svg-spinners": "^1.1.1",
    "@nuxt/content": "2.7.1",
    "@nuxt/image": "1.0.0-rc.1",
    "@nuxt/kit": "3.6.5",
    "@nuxtjs/color-mode": "3.3.0",
    "@nuxtjs/fontaine": "0.4.1",
    "@nuxtjs/html-validator": "1.5.2",
    "@nuxtjs/plausible": "0.2.1",
    "@nuxtjs/web-vitals": "0.2.6",
    "@unhead/vue": "^1.1.32",
    "@unocss/nuxt": "^0.53.6",
    "@unocss/postcss": "^0.53.6",
    "feed": "^4.2.2",
    "globby": "^13.2.2",
    "gray-matter": "4.0.3",
    "knitwork": "^1.0.0",
    "magic-regexp": "0.7.0",
    "magic-string": "^0.30.1",
    "masto": "^5.11.4",
    "mlly": "^1.4.0",
    "nuxt": "3.6.5",
    "nuxt-time": "^0.1.1",
    "partykit": "0.0.0-278833e",
    "pathe": "^1.1.1",
    "postcss-nesting": "^12.0.0",
    "prism-themes": "1.9.0",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "slugify": "^1.6.6",
    "tslib": "2.6.0",
    "ufo": "^1.1.2",
    "unplugin": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.7",
    "@commitlint/config-conventional": "17.6.7",
    "@nuxt/devtools": "0.6.7",
    "@nuxt/eslint-config": "0.1.1",
    "@nuxt/test-utils": "3.6.5",
    "@playwright/test": "1.36.2",
    "@types/node": "20.4.4",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/test-utils": "2.4.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "5.0.0",
    "happy-dom": "10.5.2",
    "lint-staged": "13.2.3",
    "nuxt-vitest": "0.10.1",
    "ofetch": "1.1.1",
    "postcss-html": "1.5.0",
    "prettier": "3.0.0",
    "simple-git-hooks": "2.8.1",
    "stylelint": "15.10.2",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "34.0.0",
    "typescript": "5.1.6",
    "vitest": "0.33.0",
    "vue-tsc": "1.8.5"
  },
  "resolutions": {
    "consola": "3.2.3",
    "semver": "< 6.0.0, >= 6.3.1"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm commitlint --edit $1",
    "pre-commit": "pnpm lint-staged"
  },
  "packageManager": "pnpm@8.6.10",
  "pnpm": {
    "patchedDependencies": {
      "@nuxtjs/color-mode@3.3.0": "patches/@nuxtjs__color-mode@3.3.0.patch",
      "h3@1.7.1": "patches/h3@1.7.1.patch"
    }
  }
}
