{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt generate",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest run test/unit",
    "test:e2e": "playwright test test/e2e",
    "postinstall": "nuxt prepare && husky install"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "npm:@nuxt/content-edge@2.1.0-27721075.4414b91",
    "@nuxt/image-edge": "1.0.0-27719579.87dcdf2",
    "@nuxt/kit": "npm:@nuxt/kit-edge@3.0.0-rc.9-27703329.537338f",
    "@nuxtjs/critters": "0.3.3",
    "nuxt-full-static": "0.2.1",
    "prism-themes": "1.9.0",
    "vue-plausible": "1.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@nuxtjs/color-mode": "3.1.6",
    "@nuxtjs/eslint-config": "11.0.0",
    "@nuxtjs/eslint-config-typescript": "11.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/tailwindcss": "5.3.3",
    "@playwright/test": "1.26.0",
    "@vitejs/plugin-vue": "3.1.0",
    "@vue/test-utils": "2.0.2",
    "c8": "7.12.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "6.0.4",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "magic-regexp": "0.5.0",
    "nuxt": "npm:nuxt3@3.0.0-rc.9-27703329.537338f",
    "postcss-html": "1.5.0",
    "prettier": "2.7.1",
    "stylelint": "14.11.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "28.0.0",
    "vitest": "0.23.4"
  },
  "packageManager": "pnpm@7.11.0"
}
