{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --fix --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest run test/unit",
    "test:types": "nuxi prepare && vue-tsc --noEmit",
    "test:e2e": "playwright test test/e2e",
    "test:e2e:update": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:v1.33.0-focal npx playwright test test/e2e --update-snapshots",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install && node ./node_modules/playwright/install.js"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/content": "2.7.0",
    "@nuxt/image": "1.0.0-rc.1",
    "@nuxt/kit": "3.6.1",
    "@nuxtjs/color-mode": "3.3.0",
    "@nuxtjs/fontaine": "0.4.0",
    "@nuxtjs/html-validator": "1.5.0",
    "@nuxtjs/plausible": "0.2.1",
    "@nuxtjs/tailwindcss": "6.8.0",
    "@nuxtjs/web-vitals": "0.2.6",
    "@unhead/vue": "^1.1.29",
    "feed": "^4.2.2",
    "globby": "^13.1.4",
    "gray-matter": "4.0.3",
    "jose": "4.14.4",
    "knitwork": "^1.0.0",
    "magic-regexp": "0.7.0",
    "magic-string": "^0.30.0",
    "mlly": "^1.4.0",
    "nuxt": "3.6.1",
    "nuxt-time": "^0.1.0",
    "pathe": "^1.1.1",
    "playwright": "1.33.0",
    "prism-themes": "1.9.0",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "slugify": "^1.6.6",
    "tslib": "2.6.0",
    "ufo": "^1.1.2",
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@nuxt/devtools": "0.6.7",
    "@nuxt/test-utils": "3.6.1",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/eslint-module": "4.1.0",
    "@playwright/test": "1.33.0",
    "@types/node": "20.4.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/test-utils": "2.4.0",
    "c8": "8.0.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "happy-dom": "9.20.3",
    "lint-staged": "13.2.3",
    "nuxt-vitest": "0.9.0",
    "ofetch": "1.1.1",
    "postcss-html": "1.5.0",
    "prettier": "2.8.8",
    "simple-git-hooks": "2.8.1",
    "stylelint": "15.10.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "34.0.0",
    "typescript": "5.1.6",
    "vitest": "0.32.2",
    "vue-tsc": "1.8.4"
  },
  "resolutions": {
    "consola": "3.2.2"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm commitlint --edit $1",
    "pre-commit": "pnpm lint-staged"
  },
  "packageManager": "pnpm@8.6.6"
}
