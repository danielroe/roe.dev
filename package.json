{
  "name": "roe.dev",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "partykit:dev": "partykit dev ./partykit/server.ts",
    "partykit:deploy": "partykit deploy ./partykit/server.ts",
    "lint:js": "eslint .",
    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "test": "vitest run test/unit/behaviour && vitest run test/unit/bundle && vitest run test/nuxt --config vitest.nuxt.config.mts",
    "test:types": "nuxt prepare && vue-tsc -b --noEmit",
    "test:e2e": "playwright test test/e2e",
    "test:e2e:update": "docker run --rm --network host -v $(pwd):/work/ -v /tmp/playwright-nm:/work/node_modules -w /work/ -it mcr.microsoft.com/playwright:v1.56.0-noble bash -c 'corepack enable && pnpm i && pnpm playwright test test/e2e --update-snapshots'",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install",
    "auth:linkedin": "node --env-file=.env scripts/linkedin-auth.ts",
    "auth:youtube": "node --env-file=.env scripts/youtube-auth.ts",
    "auth:youtube:refresh": "node --env-file=.env scripts/youtube-auth.ts refresh",
    "import-streambeats": "node --env-file=.env scripts/import-streambeats.mjs",
    "import-streambeats:dry-run": "node --env-file=.env scripts/import-streambeats.mjs --dry-run"
  },
  "lint-staged": {
    "*.{js,ts,mjs,vue}": [
      "eslint"
    ],
    "*.{css,vue}": "stylelint"
  },
  "dependencies": {
    "@atproto/api": "^0.17.0",
    "@iconify-json/ri": "^1.2.5",
    "@iconify-json/svg-spinners": "^1.2.2",
    "@iconify-json/tabler": "^1.2.18",
    "@nuxt/content": "3.7.1",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/fonts": "0.11.4",
    "@nuxt/image": "2.0.0",
    "@nuxt/scripts": "^0.12.0",
    "@nuxtjs/color-mode": "3.5.2",
    "@nuxtjs/html-validator": "2.1.0",
    "@nuxtjs/plausible": "2.0.1",
    "@nuxtjs/sanity": "^2.0.0",
    "@portabletext/types": "^2.0.13",
    "@sanity/client": "^7.8.1",
    "@sanity/webhook": "^4.0.4",
    "@unhead/vue": "2.0.19",
    "@unocss/nuxt": "^66.1.3",
    "@unocss/postcss": "^66.1.3",
    "@unocss/reset": "^66.1.3",
    "@vercel/blob": "^2.0.0",
    "citty": "^0.1.6",
    "defu": "^6.1.4",
    "exsolve": "^1.0.5",
    "feed": "^5.1.0",
    "globby": "^15.0.0",
    "gray-matter": "4.0.3",
    "h3": "^1.15.3",
    "image-meta": "^0.2.1",
    "knitwork": "^1.2.0",
    "magic-regexp": "0.10.0",
    "magic-string": "^0.30.17",
    "masto": "^7.1.0",
    "mlly": "^1.7.4",
    "mocked-exports": "^0.1.1",
    "nuxt": "^4.0.0-alpha.1",
    "nuxt-og-image": "5.1.11",
    "nuxt-security": "2.4.0",
    "partykit": "0.0.115",
    "partysocket": "^1.1.4",
    "pathe": "^2.0.3",
    "pkg-types": "^2.3.0",
    "postcss-nesting": "^13.0.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "scule": "^1.3.0",
    "std-env": "^3.9.0",
    "ufo": "^1.6.1",
    "unocss": "^66.1.3",
    "unplugin": "^2.3.5",
    "vue": "3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@nuxt/test-utils": "3.19.2",
    "@playwright/test": "1.56.0",
    "@sanity/eslint-config-studio": "5.0.2",
    "@vue/test-utils": "2.4.6",
    "eslint": "9.38.0",
    "googleapis": "164.1.0",
    "happy-dom": "20.0.8",
    "lint-staged": "16.2.4",
    "ofetch": "1.4.1",
    "playwright-core": "1.56.0",
    "rollup": "4.52.4",
    "simple-git-hooks": "2.13.1",
    "stylelint": "16.25.0",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "39.0.1",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "vue-tsc": "3.1.1"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npx lint-staged"
  },
  "packageManager": "pnpm@10.21.0",
  "pnpm": {
    "overrides": {
      "@nuxt/kit": "4.1.3",
      "@nuxt/schema": "4.1.3",
      "@nuxtjs/mdc": "0.18.0",
      "vue": "3.5.22"
    },
    "patchedDependencies": {
      "@nuxt/image": "patches/@nuxt__image.patch"
    }
  }
}
